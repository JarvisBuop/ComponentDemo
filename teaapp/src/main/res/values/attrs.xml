<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--custom-series-->
    <declare-styleable name="CustomSeekViewGroup">
        <attr name="leftName" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomMyItem">
        <attr name="leftIcon" format="reference" />
        <attr name="rightIcon" format="reference" />
        <attr name="middleName" format="string" />
        <attr name="middleTxt_color" format="color" />
        <attr name="middleTxt_dimen" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomUploadFile">
        <attr name="txt_topline" format="string" />
        <attr name="txt_toptitle" format="string" />
        <attr name="btn_middleUpload" format="string" />

        <attr name="layout_ifnot" format="boolean"/>
        <attr name="layout_first" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomUploadFileStyle2">
        <attr name="txt_topline2" format="string" />
        <attr name="txt_edit_label" format="string" />
        <attr name="edit_hint" format="string" />

        <attr name="txt_btn_label" format="string"/>
        <attr name="txt_thing" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomGroupTxtImg">
        <attr name="txt_bottom" format="string" />
        <attr name="img_top" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomLogView" >
        <attr name="editHint" format="string" />
        <attr name="editFocus" format="boolean" />
        <attr name="left_icon" format="reference" />
        <attr name="right_txt" format="string" />
        <attr name="input_mode" format="enum">
            <enum name="text" value="0" />
            <enum name="icon" value="1" />
        </attr>
        <attr name="inputType" format="enum">
            <enum name="text" value="0"/>
            <enum name="psd" value="1"/>
        </attr>

        <attr name="right_txt_color" format="color"/>
    </declare-styleable>
</resources>

