<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/padding_nor_5dp"
    android:paddingRight="@dimen/padding_nor_5dp"
    tools:background="@color/white">

    <TextView
        android:id="@+id/txt_data_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:minWidth="120dp"
        android:padding="@dimen/padding_nor_8dp"
        android:textColor="@color/text_nor"
        android:textSize="@dimen/dimen_size_18sp"
        tools:text="111111" />

    <ImageView
        android:id="@+id/img_data_select"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="@dimen/margin_nor_5dp"
        android:src="@mipmap/icon_calendar_choose" />
</LinearLayout>
